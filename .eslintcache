'react-redux';\nimport { createStore, applyMiddleware } from 'redux';\nimport reduxPromise from 'redux-promise';\nimport reducers from './reducers'\n\n\n\nexport default ({children, initialState={}}) => {\n  const store = createStore(reducers, initialState, applyMiddleware(reduxPromise));\n  return (\n    <Provider store={store}>\n      {children}\n    </Provider>\n  )\n};","/Users/windingpath17/personalproject/mnemosyne/src/components/App.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/reducers/index.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/actions/index.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/actions/types.js",[],["93","94"],"/Users/windingpath17/personalproject/mnemosyne/src/components/CardContainer.js",["95"],"import React, { useEffect, useState } from 'react';\nimport { connect } from 'react-redux';\nimport * as actions from '../actions'\nimport { useParams } from 'react-router-dom';\nimport Controls from './Controls';\nimport Card from './Card'\n\n\nconst CardContainer = (props) => {\n    const params = useParams();\n    const isParts = params.name === 'parts'\n    const cardList = isParts? props.parts : props.vocab\n    const [index, setIndex] = useState(0)\n\n    const nextCard = () => setIndex(index + 1)\n    const prevCard = () => setIndex(index - 1)\n\n    return (\n    <div>\n        {renderCards(cardList, isParts, index)}\n        <Controls cardIndex={index} listLength={cardList.length} next={nextCard} prev={prevCard}/>\n    </div>)\n}\n\n\nfunction renderCards(cardData, parts, index) {\n    //parts is a boolean\n    if (cardData.length > 0) {\n        if (parts) {\n            let cardInfo = createCards(cardData[index])\n            return <Card front={cardInfo.front} def={cardInfo.def} otherParts={cardInfo.otherParts}/>\n        }\n        else {\n            console.log(cardData)\n            return <Card front={cardData[index].word} def={cardData[index].def_en}/>\n        }  \n    }\n    else {\n        return null\n    }\n\n}\n\nfunction createCards(card) {\n    const info = {\n        id: card.id, \n        front: card.present, \n        otherParts: [card.future, card.aorist, card.perfect, card.prf_mid_pass, card.aor_pass],\n        def: card.def_en\n    }\n    return info\n}\n\nfunction mapStateToProps(state) {\n    return {  \n        parts: state.parts,\n        vocab: state.vocab,\n        index: state.cardIndex\n    }\n}\n\nexport default connect(mapStateToProps, actions)(CardContainer);","/Users/windingpath17/personalproject/mnemosyne/src/components/Controls.js",["96","97"],"import React from 'react';\nimport { connect } from 'react-redux';\nimport * as actions from '../actions'\nimport styles from './Controls.module.css'\n\nconst Controls = (props) =>{\n    return (\n        <div className={styles.control_container}>\n            <button disabled={props.cardIndex === 0} onClick={props.prev}>back</button>\n            <span>{`< ${props.cardIndex + 1} / ${props.listLength} >`}</span>\n            <button disabled={(props.cardIndex+1) === props.listLength} onClick={props.next}>next</button>\n        </div>\n    )\n\n}\n\nexport default Controls;","/Users/windingpath17/personalproject/mnemosyne/src/reducers/cardIndex.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/components/FolderContainer.js",["98"],"import React from 'react';\nimport { connect } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport * as actions from '../actions'\nimport Folder from './Folder';\n\nclass FolderContainer extends React.Component {\n    constructor(props) {\n        super(props);\n        this.renderFolders = this.renderFolders.bind(this);\n        this.handleClick = this.handleClick.bind(this)\n    }\n    componentDidMount(){\n    //fetches the list of folders from API\n    this.props.fetchFolders()\n  }\n\n  componentWillUnmount() {\n      this.props.clearFolders()\n  }\n\n  renderFolders() {\n      //renders folders from list\n    if (this.props.folderList.length > 0) {\n        return this.props.folderList.map((folder, index) => \n        <Folder key={index} name={folder.folder_name} folder_id={folder.id}/>)\n    }\n    else {\n        return null\n    }\n  }\n\n  \n  handleClick(){\n        this.props.fetchParts()\n        this.props.history.push('/set/parts')\n    }\n\n  render() {\n      return (\n        <div>\n              {this.renderFolders()}\n            \n            <div className='card' onClick={this.handleClick}>\n                <p>Principal Parts</p>\n            </div>\n        </div>\n        \n      )\n  }\n}\n\nfunction mapStateToProps(state) {\n    return {\n      folderList: state.folders\n    }\n  }\n\nexport default connect(mapStateToProps, actions)(FolderContainer);","/Users/windingpath17/personalproject/mnemosyne/src/components/SetContainer.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/reducers/folders.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/reducers/sets.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/components/Folder.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/components/Set.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/reducers/vocab.js",[],"/Users/windingpath17/personalproject/mnemosyne/src/reducers/parts.js",["99"],"import { CREATE_CARDS, FETCH_PARTS } from '../actions/types'\n\nexport default function(state=[], action) {\n    switch(action.type) {\n        case FETCH_PARTS:\n            return [...state, ...action.payload.data];\n        case CREATE_CARDS:\n            return []\n        default:\n            return state\n    }\n}","/Users/windingpath17/personalproject/mnemosyne/src/components/PartsCard.js",["100","101","102","103","104","105","106"],"/Users/windingpath17/personalproject/mnemosyne/src/components/VocabCard.js",[],["107","108"],"/Users/windingpath17/personalproject/mnemosyne/src/components/Card.js",[],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","